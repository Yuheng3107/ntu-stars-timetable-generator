{
  "manifest_version": 3,
  "name": "NTU Stars Calendar Generator",
  "version": "1.0",
  "description": "Generates a CSV file format from NTU Stars Page that can be imported into Google Calendar",
  "permissions": ["downloads"],
  "content_scripts": [
    {
      "js": ["./parse.js"],
      "matches": [
        "https://wish.wis.ntu.edu.sg/pls/webexe/AUS_STARS_PLANNER.planner"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
